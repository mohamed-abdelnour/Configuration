Xft.dpi: 120
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

Xcursor.theme: default

! special
*.foreground:   #ABB2BF
*.background:   #282C34
*.cursorColor:  #ABB2BF

! black
*.color0:       #282C34
*.color8:       #5C6370

! red
*.color1:       #E06C75
*.color9:       #E06C75

! green
*.color2:       #98C379
*.color10:      #98C379

! yellow
*.color3:       #E5C07B
*.color11:      #E5C07B

! blue
*.color4:       #61AFEF
*.color12:      #61AFEF

! magenta
*.color5:       #C678DD
*.color13:      #C678DD

! cyan
*.color6:       #56B6C2
*.color14:      #56B6C2

! white
*.color7:       #ABB2BF
*.color15:      #ABB2BF

! Xmessage
Xmessage*font: -xos4-terminus-medium-r-normal--22-220-72-72-c-110-iso10646-1

! Emacs
emacs.borderColor: #282C34
emacs.font: Inconsolata-12:width=semiexpanded
emacs.horizontalScrollBars: off
emacs.internalBorder: 6
emacs.menuBar: off
emacs.scrollBar: off
emacs.toolBar: off
emacs.verticalScrollBars: off
